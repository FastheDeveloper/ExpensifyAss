    "@testing-library/react-native": "^12.6.0",
    "@tsconfig/react-native": "^3.0.0",
    "@types/node": "^22.4.0",
    "@types/react": "~18.2.45",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^7.7.0",
    "@typescript-eslint/parser": "^7.7.0",
